  "scripts": {
    "build": "parcel build src/index.pug -d public --no-minify --cache-dir node_modules/.cache/parcel",
    "build:prod": "del-cli public; npm run data && parcel build src/index.pug -d public --no-content-hash --no-source-maps --no-cache",
